{
  "name": "vpgrp-influxdb",
  "version": "0.0.3",
  "author": "vpgrp",
  "summary": "Puppet module to install, deploy and configure influxdb.",
  "license": "Apache-2.0",
  "source": "https://git.vpgrp.io/puppet/puppet-influxdb",
  "project_page": "https://git.vpgrp.io/puppet/puppet-influxdb",
  "issues_url": "https://git.vpgrp.io/puppet/puppet-influxdb/issues",
