{
  "name": "vpgrp-influxdb",
  "version": "1.3.2",
  "author": "vpgrp",
  "summary": "Puppet module to install, deploy and configure InfluxDB.",
  "license": "Apache-2.0",
  "source": "https://github.com/vente-privee/puppet-influxdb",
  "project_page": "https://github.com/vente-privee/puppet-influxdb",
  "issues_url": "https://github.com/vente-privee/puppet-influxdb/issues",
  "dependencies": [
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 4.5.1 <= 7.0.0"
    }
